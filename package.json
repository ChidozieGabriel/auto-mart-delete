{
  "name": "auto-mart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "rm -rf dist",
    "lint-fix": "./node_modules/.bin/eslint --fix src/**/*.js",
    "lint-fix-dry": "./node_modules/.bin/eslint --fix-dry-run src/**/*.js",
    "start": "NODE_ENV=prod DEBUG=http node ./dist",
    "start-dev": "NODE_ENV=dev DEBUG=http babel-watch ./src",
    "test": "NODE_ENV=test nyc mocha --timeout 4000 --recursive --exit --require @babel/register --require @babel/polyfill './src/**/**test.js'",
    "test-coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "async": "^3.0.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "datauri": "^2.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "pg": "^7.11.0",
    "swagger-ui-express": "^4.0.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-watch": "^7.0.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  },
  "browserslist": {
    "production": [
      "> 0.25%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
